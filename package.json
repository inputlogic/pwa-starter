{
  "name": "pwa-starter",
  "version": "1.0.0",
  "description": "PWA Starter with Preact",
  "main": "server/index.js",
  "heroku-run-build-script": true,
  "scripts": {
    "preinstall": "if test -z \"$NODE_ENV\"; then rm .git/hooks/pre-commit; cp pre-commit.sh .git/hooks/pre-commit; else rm -rf public; mkdir public; fi",
    "start": "if test -z \"$NODE_ENV\"; then npm run dev; else npm run browse; fi",
    "dev": "npm-run-all --parallel build:dev watch-assets",
    "prebuild:dev": "npm run lint --silent",
    "build:dev": "rollup -c config/dev.js -w",
    "watch-assets": "sane 'npm run prelint' src/assets",
    "browse": "node server/index.js",
    "prebuild": "npm run test",
    "build": "rollup -c config/prod.js",
    "prelint": "rm -rf public; mkdir public; cp -r src/assets/* public/; cp -r src/assets/* public/",
    "lint": "eslint ./src",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint ./src"
    ]
  },
  "reactSnap": {
    "source": "public",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [],
  "author": "Adrian Unger",
  "license": "MIT",
  "dependencies": {
    "@app-elements/avatar": "2.0.1",
    "@app-elements/carousel": "2.0.1",
    "@app-elements/dropdown": "4.0.1",
    "@app-elements/equal": "^1.0.4",
    "@app-elements/helmet": "3.0.3",
    "@app-elements/image": "2.0.0",
    "@app-elements/loading-indicator": "2.0.1",
    "@app-elements/modal": "4.0.1",
    "@app-elements/notification": "3.0.1",
    "@app-elements/router": "3.2.1",
    "@app-elements/use-form": "^0.2.1",
    "@app-elements/use-mapped-state": "^1.0.0",
    "@app-elements/use-request": "2.1.1",
    "@app-elements/use-store-path": "^1.0.1",
    "atom": "1.4.1",
    "compression": "1.7.4",
    "polka": "0.5.2",
    "preact": "10.4.5",
    "react-transition-group": "^4.4.1",
    "sirv": "1.0.1",
    "wasmuth": "2.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-spread": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-babel": "5.0.4",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-inject": "4.0.2",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-run": "2.0.2",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "eslint": "7.4.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.6",
    "eslint-plugin-standard": "4.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "less": "3.11.3",
    "less-plugin-glob": "^3.0.0",
    "lint-staged": "10.2.11",
    "npm-run-all": "4.1.5",
    "react-snap": "1.23.0",
    "rollup": "2.21.0",
    "rollup-plugin-css-only": "2.1.0",
    "rollup-plugin-dev": "1.1.2",
    "rollup-plugin-livereload": "1.3.0",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-terser": "6.1.0",
    "sane": "4.1.0"
  }
}
