{
  "presets": [
    ["env", {
      "modules": false
    }],
    "react"
  ],
  "plugins": [
    "external-helpers",
    "syntax-dynamic-import",
    "transform-es2015-spread",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }],
    ["transform-react-jsx", {"pragma": "h"}],
    ["../node_modules/babel-plugin-jsx-import/src/index.js", {
      "identifier": "h",
      "moduleName": "preact",
      "isNamed": true
    }],
  ]
}
